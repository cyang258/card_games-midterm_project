<!DOCTYPE html>
<html lang="en">
  <head>
    <title>User Page</title>

    <%- include partials/_head.ejs %>

    <!-- Layouts CSS -->
    <link rel="stylesheet" href="/styles/users.css" type="text/css" />

    <!-- Users_games script -->
    <script type="text/javascript" src="/scripts/user-history.js"></script>

    <!-- Socket IO -->
    <script src="https://cdnjs.cloudflare.com/ajax/libs/socket.io/2.0.4/socket.io.js"></script>
  </head>

  <body>
    <div class="content">
      <%- include partials/_header.ejs %>

      <main>
        <div class="spacer">
        </div>

        <nav class="games">
          <h3>Username: <span class="username"></span></h3>
          <button>Goofspiel</button>
          </nav>
        </nav>

        <table class="history">
          <tr>
            <th>Game</th>
            <th>Game Id</th>
            <th>Start</th>
            <th>End</th>
            <th>Opponents</th>
            <th>Score</th>
          </tr>
        </table>
      </main>
    </div>

    <footer class="footer">
      <h6>Created 2017</h6>
    </footer>
  </body>
</html>
